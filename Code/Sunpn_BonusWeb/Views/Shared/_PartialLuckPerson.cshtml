
@using Sunpn_BonusWeb.Models

@if (ViewBag.perlist != null)
{
    List<isPer> perlist = ViewBag.perlist;
    foreach (var item in perlist)
    {
        <tr id="tb" style="border:1px solid #dedada" class="tr">
            <td class="selectUser">@item.Uid</td>
            <td>@item.Name</td>
            <td>
                <input type="hidden" value="@item.IsEmp" class="isEmp"/>
                <img src="~/Content/Images/del.png" class="delPerson" width="24" height="24" />

            </td>
        </tr>
    }
}





